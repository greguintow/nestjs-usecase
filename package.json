{
  "name": "@greguintow/nestjs-usecase",
  "version": "9.0.1",
  "description": "A lightweight CQRS module for Nest framework (node.js)",
  "license": "MIT",
  "url": "https://github.com/nestjs/cqrs#readme",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepublish:next": "yarn build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "yarn build",
    "publish:npm": "npm publish --access public",
    "prerelease": "yarn build",
    "release": "release-it",
    "prepare": "husky install"
  },
  "dependencies": {
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "^17.2.0",
    "@nestjs/common": "9.1.6",
    "@nestjs/core": "9.1.6",
    "@types/jest": "29.2.0",
    "@types/node": "16.18.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "^44.0.2",
    "husky": "8.0.1",
    "jest": "29.2.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "reflect-metadata": "0.1.13",
    "release-it": "15.5.0",
    "rxjs": "7.5.7",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.2.0"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greguintow/nestjs-usecase"
  }
}
